{
  "name": "docusaurus-starter",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/WebDevStudios/docusaurus-starter/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebDevStudios/docusaurus-starter.git"
  },
  "author": "WebDevStudios <contact@webdevstudios.com>",
  "bugs": {
    "url": "https://github.com/WebDevStudios/docusaurus-starter/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "build": "docusaurus build",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "dev": "npm run clear && docusaurus start",
    "docusaurus": "docusaurus",
    "format": "npx prettier '**/*.{js,jsx,ts,tsx,md,mdx,html,css,scss,json,yml}' --write || true",
    "lint": "npm run format && npm run lint:js && npm run lint:css && npm run lint:md",
    "lint:css": "npx stylelint '**/*.css' --fix || true",
    "lint:js": "npx eslint '**/*.{js,ts,jsx,tsx}' --fix || true",
    "lint:md": "npx markdownlint '**/*.md' --fix",
    "serve": "docusaurus serve",
    "start": "npm run clear && docusaurus start",
    "swizzle": "docusaurus swizzle",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.14",
    "@docusaurus/preset-classic": "2.0.0-beta.15",
    "@mdx-js/react": "^1.6.22",
    "@svgr/webpack": "^6.2.1",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "markdownlint": "^0.25.1",
    "markdownlint-cli": "^0.31.1",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "stylelint": "^14.3.0",
    "stylelint-config-standard": "^24.0.0"
  }
}
